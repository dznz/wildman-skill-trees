      <TD COLSPAN="2"><B>Legend</B></TD>
     </TR>
     <TR>
      <TD>Previous Drills</TD>
      <TD BGCOLOR="3">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</TD>
     </TR>
     <TR>
      <TD>Primary Drills</TD>
      <TD BGCOLOR="11"></TD>
     </TR>
     <TR>
      <TD>Secondary Drills</TD>
      <TD BGCOLOR="7"></TD>
     </TR>
    </TABLE>
   >];
  }
}
