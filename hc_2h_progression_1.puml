  mov_sidetoside -> mov_rockit;
  mov_rockit -> mov_outpend;
  mov_swingclean -> mov_outpend;
  mov_outsidecircle -> mov_outpend;
  mov_squat -> mov_stepbacklunge;
  mov_insidecircle -> mov_altcircle;
