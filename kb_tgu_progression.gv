  mov_cap -> mov_startstopcap;
  mov_racksquat -> mov_racklunge;
  mov_racklunge -> mov_clean2sblunge;
  mov_startstopclean -> mov_clean2sblunge;
  mov_clean2sblunge -> mov_halfkneelclean;
  mov_clean2sblunge -> mov_cleansblungeclean;
